#!/usr/bin/env python3
import os, sys, json
from pluplusch import pluplusch

sys.stdout.write('\n')
for i, dataset in enumerate(pluplusch(cache_dir = os.path.expanduser('~/.pluplusch'), proxies = {}, data = False)):
    sys.stdout.write('\r%d metadatas retrieved' % i)
