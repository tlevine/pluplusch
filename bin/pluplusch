#!/usr/bin/env python3
import datetime
import os, sys, json
from pluplusch import pluplusch

cache_dir = os.path.expanduser('~/%s/.pluplusch' % datetime.date.today().isoformat())
sys.stdout.write('\n')
for i, dataset in enumerate(pluplusch(cache_dir = cache_dir, proxies = {}, colnames = True)):
    sys.stdout.write('\r%d metadatas retrieved' % i)
